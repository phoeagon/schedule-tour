  <script type="text/javascript" >
    $('#close-flash-btn').click(function(e){
        $("#flash-msg").remove()
    })
    setTimeout(function(){
        $('#close-flash-btn').click();
    },5000);//wait 5 secs
    $('#screen').buttonset();
    var locationLock = ($('#lock-location').val()=='Off');
    $('#lock-location').toggleSwitch({
        change : function(e){
            if ( $('#lock-location').val()=='Off' )
                locationLock = false;
            else locationLock = true;
        }
    });
  </script>
